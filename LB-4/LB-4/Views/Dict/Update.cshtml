@model LB_4.Models.DB.PhoneContactModel
@{
    ViewBag.Title = "Update";
}

<h2>Update</h2>

<form method="post" action="~/Dict/UpdateSave">
    <input type="hidden" name="Id" value="@Model.Id" />
    <label>Name: <input type="text" name="Name" value="@Model.Name" /></label>
    <label>Phone: <input type="text" name="Phone" value="@Model.Phone" /></label>
    <input type="submit" />
</form>