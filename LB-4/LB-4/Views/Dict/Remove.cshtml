@model LB_4.Models.DB.PhoneContactModel
@{
    ViewBag.Title = "Remove";
}

<h2>Remove</h2>

<p>Are you sure that you want to remove <b>@Model.Name @Model.Phone</b> ?</p>

<form method="post" action="~/Dict/RemoveSave">
    <input type="hidden" name="Id" value="@Model.Id" />
    <input type="hidden" name="Name" value="@Model.Name" />
    <input type="hidden" name="Phone" value="@Model.Phone" />
    <input type="submit" />
</form>